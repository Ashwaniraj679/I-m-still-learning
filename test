class Car():
    def __init__(self,make,model,year,color):
        self.make = make
        self.model = model
        self.year = year 
        self.color = color
        
    def start_engine(self):
        print({f'{self.make} {self.model} ({self.year}) engine has started'})
    
    def stop_engine(self):
        pass
    
    def honk_honk(self):
        pass

car1 = Car("Maruti","Suzuki", 2012,"Red")
car1.start_engine()
